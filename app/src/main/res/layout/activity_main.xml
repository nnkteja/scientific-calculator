<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="wrap_content"
    android:layout_height="wrap_content" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin" tools:context=".MainActivity">

    <!--<EditText-->
        <!--android:layout_width="wrap_content"-->
        <!--android:layout_height="wrap_content"-->
        <!--android:id="@+id/tbox"-->
        <!--/>-->
    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:id="@+id/tbox"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_toLeftOf="@+id/del"
        android:layout_toStartOf="@+id/del"
        android:layout_alignBottom="@+id/del"
        android:hint="Enter an expression to compute"
        android:lines="1" />
    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="del"
        android:id="@+id/del"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />

    <Button
        style="?android:attr/buttonStyleSmall"
        android:layout_width="75dp"
        android:layout_height="wrap_content"
        android:text="sqrt"
        android:id="@+id/sqrt"
        android:layout_below="@+id/tbox"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <Button
        style="?android:attr/buttonStyleSmall"
        android:layout_width="75dp"
        android:layout_height="wrap_content"
        android:text="inv"
        android:id="@+id/inv"
        android:layout_alignBottom="@+id/sqrt"
        android:layout_toRightOf="@+id/sqrt"
        android:layout_toEndOf="@+id/sqrt" />

    <Button
        style="?android:attr/buttonStyleSmall"
        android:layout_width="75dp"
        android:layout_height="wrap_content"
        android:text="logbase"
        android:id="@+id/logbase"
        android:layout_alignBottom="@+id/inv"
        android:layout_toRightOf="@+id/inv"
        android:layout_toEndOf="@+id/inv" />

    <Button
        style="?android:attr/buttonStyleSmall"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="power"
        android:id="@+id/power"
        android:layout_below="@+id/inv"
        android:layout_toRightOf="@+id/sqrt"
        android:layout_alignRight="@+id/inv"
        android:layout_alignEnd="@+id/inv" />

    <Button
        style="?android:attr/buttonStyleSmall"
        android:layout_width="75dp"
        android:layout_height="wrap_content"
        android:text="10^"
        android:id="@+id/pow10"
        android:layout_below="@+id/tbox"
        android:layout_toLeftOf="@+id/acos"
        android:layout_toStartOf="@+id/acos" />

    <Button
        style="?android:attr/buttonStyleSmall"
        android:layout_width="75dp"
        android:layout_height="wrap_content"
        android:text="mod"
        android:id="@+id/mod"
        android:layout_above="@+id/ln"
        android:layout_alignLeft="@+id/acos"
        android:layout_alignStart="@+id/acos"
        android:layout_alignRight="@+id/acos"
        android:layout_alignEnd="@+id/acos" />

    <Button
        style="?android:attr/buttonStyleSmall"
        android:layout_width="75dp"
        android:layout_height="wrap_content"
        android:text="squared"
        android:id="@+id/squared"
        android:layout_below="@+id/sqrt"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <Button
        style="?android:attr/buttonStyleSmall"
        android:layout_width="75dp"
        android:layout_height="wrap_content"
        android:text="log"
        android:id="@+id/log"
        android:layout_below="@+id/logbase"
        android:layout_alignLeft="@+id/tan"
        android:layout_alignStart="@+id/tan" />

    <Button
        style="?android:attr/buttonStyleSmall"
        android:layout_width="75dp"
        android:layout_height="wrap_content"
        android:text="ln"
        android:id="@+id/ln"
        android:layout_below="@+id/logbase"
        android:layout_toRightOf="@+id/log"
        android:layout_toEndOf="@+id/log" />

    <Button
        style="?android:attr/buttonStyleSmall"
        android:layout_width="75dp"
        android:layout_height="wrap_content"
        android:text="atan"
        android:id="@+id/atan"
        android:layout_below="@+id/ln"
        android:layout_toRightOf="@+id/asin"
        android:layout_toEndOf="@+id/asin" />

    <Button
        style="?android:attr/buttonStyleSmall"
        android:layout_width="75dp"
        android:layout_height="wrap_content"
        android:text="sin"
        android:id="@+id/sin"
        android:layout_below="@+id/acos"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <Button
        style="?android:attr/buttonStyleSmall"
        android:layout_width="75dp"
        android:layout_height="wrap_content"
        android:text="cos"
        android:id="@+id/cos"
        android:layout_alignTop="@+id/sin"
        android:layout_toRightOf="@+id/sin"
        android:layout_toEndOf="@+id/sin" />

    <Button
        style="?android:attr/buttonStyleSmall"
        android:layout_width="75dp"
        android:layout_height="wrap_content"
        android:text="tan"
        android:id="@+id/tan"
        android:layout_alignTop="@+id/cos"
        android:layout_toRightOf="@+id/cos"
        android:layout_toEndOf="@+id/cos" />

    <Button
        style="?android:attr/buttonStyleSmall"
        android:layout_width="75dp"
        android:layout_height="wrap_content"
        android:text="asin"
        android:id="@+id/asin"
        android:layout_alignTop="@+id/tan"
        android:layout_toRightOf="@+id/tan"
        android:layout_toEndOf="@+id/tan" />

    <Button
        style="?android:attr/buttonStyleSmall"
        android:layout_width="75dp"
        android:layout_height="wrap_content"
        android:text="abs"
        android:id="@+id/abs"
        android:layout_below="@+id/sin"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <Button
        style="?android:attr/buttonStyleSmall"
        android:layout_width="75dp"
        android:layout_height="wrap_content"
        android:text="fact!"
        android:id="@+id/fact"
        android:layout_below="@+id/sin"
        android:layout_toRightOf="@+id/abs"
        android:layout_toEndOf="@+id/abs" />

    <Button
        style="?android:attr/buttonStyleSmall"
        android:layout_width="75dp"
        android:layout_height="wrap_content"
        android:text="("
        android:id="@+id/leftpar"
        android:layout_below="@+id/tan"
        android:layout_alignLeft="@+id/tan"
        android:layout_alignStart="@+id/tan" />

    <Button
        style="?android:attr/buttonStyleSmall"
        android:layout_width="75dp"
        android:layout_height="wrap_content"
        android:text=")"
        android:id="@+id/rightpar"
        android:layout_below="@+id/tan"
        android:layout_toRightOf="@+id/leftpar"
        android:layout_toEndOf="@+id/leftpar" />

    <Button
        style="?android:attr/buttonStyleSmall"
        android:layout_width="75dp"
        android:layout_height="wrap_content"
        android:text="acos"
        android:id="@+id/acos"
        android:layout_below="@+id/mod"
        android:layout_alignLeft="@+id/atan"
        android:layout_alignStart="@+id/atan" />

    <Button
        style="?android:attr/buttonStyleSmall"
        android:layout_width="75dp"
        android:layout_height="wrap_content"
        android:text="epow"
        android:id="@+id/epow"
        android:layout_below="@+id/atan"
        android:layout_alignLeft="@+id/atan"
        android:layout_alignStart="@+id/atan" />

    <Button
        style="?android:attr/buttonStyleSmall"
        android:layout_width="75dp"
        android:layout_height="wrap_content"
        android:text="7"
        android:id="@+id/seven"
        android:layout_below="@+id/epow"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <Button
        style="?android:attr/buttonStyleSmall"
        android:layout_width="75dp"
        android:layout_height="wrap_content"
        android:text="8"
        android:id="@+id/eight"
        android:layout_alignTop="@+id/seven"
        android:layout_toRightOf="@+id/seven"
        android:layout_toEndOf="@+id/seven" />

    <Button
        style="?android:attr/buttonStyleSmall"
        android:layout_width="75dp"
        android:layout_height="wrap_content"
        android:text="9"
        android:id="@+id/nine"
        android:layout_alignTop="@+id/eight"
        android:layout_toRightOf="@+id/eight"
        android:layout_toEndOf="@+id/eight" />

    <Button
        style="?android:attr/buttonStyleSmall"
        android:layout_width="75dp"
        android:layout_height="wrap_content"
        android:text=","
        android:id="@+id/comma"
        android:layout_below="@+id/epow"
        android:layout_toRightOf="@+id/nine"
        android:layout_toEndOf="@+id/nine" />

    <Button
        style="?android:attr/buttonStyleSmall"
        android:layout_width="75dp"
        android:layout_height="wrap_content"
        android:text="AC"
        android:id="@+id/ac"
        android:layout_below="@+id/epow"
        android:layout_toRightOf="@+id/comma"
        android:layout_toEndOf="@+id/comma" />

    <Button
        style="?android:attr/buttonStyleSmall"
        android:layout_width="75dp"
        android:layout_height="wrap_content"
        android:text="4"
        android:id="@+id/four"
        android:layout_below="@+id/seven"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <Button
        style="?android:attr/buttonStyleSmall"
        android:layout_width="75dp"
        android:layout_height="wrap_content"
        android:text="5"
        android:id="@+id/five"
        android:layout_below="@+id/seven"
        android:layout_toRightOf="@+id/seven"
        android:layout_toEndOf="@+id/seven" />

    <Button
        style="?android:attr/buttonStyleSmall"
        android:layout_width="75dp"
        android:layout_height="wrap_content"
        android:text="6"
        android:id="@+id/six"
        android:layout_below="@+id/nine"
        android:layout_alignLeft="@+id/nine"
        android:layout_alignStart="@+id/nine" />

    <Button
        style="?android:attr/buttonStyleSmall"
        android:layout_width="75dp"
        android:layout_height="wrap_content"
        android:text="*"
        android:id="@+id/mul"
        android:layout_below="@+id/nine"
        android:layout_toRightOf="@+id/six"
        android:layout_toEndOf="@+id/six" />

    <Button
        style="?android:attr/buttonStyleSmall"
        android:layout_width="75dp"
        android:layout_height="wrap_content"
        android:text="/"
        android:id="@+id/div"
        android:layout_below="@+id/ac"
        android:layout_alignLeft="@+id/ac"
        android:layout_alignStart="@+id/ac" />

    <Button
        style="?android:attr/buttonStyleSmall"
        android:layout_width="75dp"
        android:layout_height="wrap_content"
        android:text="1"
        android:id="@+id/one"
        android:layout_below="@+id/four"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <Button
        style="?android:attr/buttonStyleSmall"
        android:layout_width="75dp"
        android:layout_height="wrap_content"
        android:text="2"
        android:id="@+id/two"
        android:layout_below="@+id/four"
        android:layout_toRightOf="@+id/four"
        android:layout_toEndOf="@+id/four" />

    <Button
        style="?android:attr/buttonStyleSmall"
        android:layout_width="75dp"
        android:layout_height="wrap_content"
        android:text="3"
        android:id="@+id/three"
        android:layout_below="@+id/six"
        android:layout_alignLeft="@+id/six"
        android:layout_alignStart="@+id/six" />

    <Button
        style="?android:attr/buttonStyleSmall"
        android:layout_width="75dp"
        android:layout_height="wrap_content"
        android:text="+"
        android:id="@+id/plus"
        android:layout_below="@+id/six"
        android:layout_toRightOf="@+id/three"
        android:layout_toEndOf="@+id/three" />

    <Button
        style="?android:attr/buttonStyleSmall"
        android:layout_width="75dp"
        android:layout_height="wrap_content"
        android:text="-"
        android:id="@+id/minus"
        android:layout_below="@+id/div"
        android:layout_alignLeft="@+id/div"
        android:layout_alignStart="@+id/div" />

    <Button
        style="?android:attr/buttonStyleSmall"
        android:layout_width="75dp"
        android:layout_height="wrap_content"
        android:text="00"
        android:id="@+id/zeros"
        android:layout_below="@+id/one"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <Button
        style="?android:attr/buttonStyleSmall"
        android:layout_width="75dp"
        android:layout_height="wrap_content"
        android:text="0"
        android:id="@+id/zero"
        android:layout_below="@+id/one"
        android:layout_toRightOf="@+id/one"
        android:layout_toEndOf="@+id/one" />

    <Button
        style="?android:attr/buttonStyleSmall"
        android:layout_width="75dp"
        android:layout_height="wrap_content"
        android:text="."
        android:id="@+id/dot"
        android:layout_below="@+id/two"
        android:layout_toRightOf="@+id/zero"
        android:layout_toEndOf="@+id/zero" />

    <Button
        style="?android:attr/buttonStyleSmall"
        android:layout_width="75dp"
        android:layout_height="wrap_content"
        android:text="%"
        android:id="@+id/percent"
        android:layout_below="@+id/three"
        android:layout_toRightOf="@+id/three"
        android:layout_toEndOf="@+id/three" />

    <Button
        style="?android:attr/buttonStyleSmall"
        android:layout_width="75dp"
        android:layout_height="wrap_content"
        android:text="="
        android:id="@+id/equals"
        android:layout_below="@+id/minus"
        android:layout_alignLeft="@+id/minus"
        android:layout_alignStart="@+id/minus" />

    <ScrollView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/scrollView"
        android:layout_alignParentTop="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:fillViewport="true" >


    </ScrollView>

</RelativeLayout>
